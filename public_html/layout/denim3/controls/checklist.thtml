{# begin {templatelocation} #}

<ul class="checkboxes-list">
    {items}
</ul>

<!-- BEGIN item -->
<li><label><input type="checkbox"{checked} name="{name}" class="uk-check" value="{value}"{xhtml}><span>{label}</span></label></li>
<!-- END item -->

<!-- BEGIN item-default -->
<li><label><input type="checkbox"{checked} name="{name}" class="uk-check" value="{value}"{xhtml}><span class="gldefault">{label}</span></label></li>
<!-- END item-default -->

{# end {templatelocation} #}