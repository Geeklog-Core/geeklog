
<!-- trackback/formattedcomment.thtml { -->

<div class="formattedcomment">
  <ul class="status">
    <li class="title"><a href="{trackback_url}">{trackback_title}</a>&nbsp;{trackback_from_blog_name}</li>
    <li class="trackback_date">{lang_tracked_on}&nbsp;{trackback_date}</li>
  </ul>

  <p class="trackback_excerpt_readmore">{trackback_excerpt_readmore}</p>

  <ul class="navi">
    <li class="delete_option">{delete_option}</li>
  </ul>
</div>

<!-- } trackback/formattedcomment.thtml -->
